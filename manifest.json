{
  "name": "Couchpotato",
  "id": "couchpotato",
  "description": {
    "en": "Manage movie collection",
    "fr": "Gestion de filmoth√®que"
  },
  "developer": {
    "name": "lunarok",
    "email": "cedric@wayang.fr",
    "url": "http://www.wayang.fr"
  },
  "multi_instance": "false",
  "arguments": {
    "install": [
      {
        "name": "domain",
        "ask": {
          "en": "Choose a domain for Couchpotato"
        },
        "example": "domain.org"
      },
      {
        "name": "path",
        "ask": {
          "en": "Choose a path for Couchpotato"
        },
        "example": "/films",
        "default": "/films"
      },
      {
        "name": "server",
        "ask": {
          "en": "Choose IP where Couchpotato is installed"
        },
        "example": "127.0.0.1",
        "default": "127.0.0.1"
      }
    ]
  }
}
